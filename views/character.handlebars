<div class="w3-row">
    <div class="w3-col m12 l12 w3-margin">
        <div class="w3-container w3-red">
            {{#if character}}
            {{> getCharacter character}}
            {{else}}
            <p>Create a character! Created Characters will be here.</p>
            {{/if}}
        </div>
    </div>


    <div class="w3-col m12 l12 w3-margin">
        <div class="w3-container w3-red">
            {{> createCharacter user_id}}
        </div>
    </div>
</div>